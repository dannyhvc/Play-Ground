<Window x:Class="GUI1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GUI1"
        mc:Ignorable="d"
        Title="MainWindow" Height="230" Width="189.5">

    <WrapPanel HorizontalAlignment="Left" Width="132" Margin="28,0,0,2.5">
        <WrapPanel Height="170" Width="120">
            <Label Content="Path" Width="120" />
            <TextBox Height="23" TextWrapping="Wrap" Text="TextBox" Width="120" TextChanged="TextBox_TextChanged_1" />
            <Separator Margin="0" Width="120" Height="10" Opacity="0" />
            <Label Content="Password" Width="120" />
            <TextBox Height="23" TextWrapping="Wrap" Text="TextBox" Width="120" />
            <Separator Margin="0" Width="120" Height="15" Opacity="0" />
            <Button Content="Button" Width="120" />
            <ProgressBar Height="22" Width="120" />
        </WrapPanel>
    </WrapPanel>
</Window>